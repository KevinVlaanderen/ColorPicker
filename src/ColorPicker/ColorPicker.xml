<?xml version="1.0" encoding="utf-8" ?>
<widget id="ColorPicker.widget.ColorPicker" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
  <name>ColorPicker</name>
  <description>Adds a color picker input field to your dataview.</description>
  <icon>

</icon>
  <properties>
    <property key="Entity" type="entity">
      <caption>Entity </caption>
      <category>Data source</category>
      <description>The Entity which holds the color attribute.</description>
    </property>
	<property key="ColorAttribute" type="attribute" entityProperty="Entity">
      <caption>Color Attribute</caption>
      <category>Data source</category>
      <description>The attribute that holds the formatted color value.</description>
      <attributeTypes>
        <attributeType name="String"/>
      </attributeTypes>
    </property>
    <property key="onChangeMF" type="microflow" required="false" defaultValue="" entityProperty="Entity">
            <caption>On Change Microflow</caption>
            <category>Interaction</category>
            <description>Microflow that will be executed when the color picker widget is closed. Takes the Entity chosen in the Data Source tab as input parameter.</description>
            <returnType type="Void"></returnType>
    </property>
	<property key="horizontal" type="boolean" defaultValue="true">
            <caption>Horizontal</caption>
            <category>Appearance</category>
            <description>If set to 'Yes', the hue and alpha channel bars will be rendered horizontally, above the saturation selector.</description>
    </property>
	<property key="disable" type="boolean" defaultValue="false">
            <caption>Disable</caption>
            <category>Appearance</category>
            <description>If set to 'Yes', the color picker can not be triggered. Typically used in non-editable scenario's.</description>
    </property>
	<property key="inline" type="boolean" defaultValue="false">
            <caption>Inline</caption>
            <category>Appearance</category>
            <description>If set to 'Yes', forces to show the colorpicker as an inline element.</description>
    </property>
	<property key="align" type="enumeration" required="true" defaultValue="right">
            <caption>Align</caption>
            <category>Appearance</category>
            <description>By default, the colorpicker is aligned to the right of the input. If you need to switch it to the left, set align to 'left'.</description>
			<enumerationValues>
				<enumerationValue key="right">Right</enumerationValue>
				<enumerationValue key="left">Left</enumerationValue>
			</enumerationValues>
	</property>
	<property key="format" type="enumeration" required="true" defaultValue="false">
		<caption>Format</caption>
		<category>Appearance</category>
		<description>This setting enforces formats if the chosen format is hex, rgb or rgba. When 'Format' is chosen the format is automatically detected.</description>
		<enumerationValues>
			<enumerationValue key="false">False</enumerationValue>
			<enumerationValue key="hex">Hex</enumerationValue>
			<enumerationValue key="rgba">Rgba</enumerationValue>
			<enumerationValue key="rgb">rgb</enumerationValue>
		</enumerationValues>
	</property>
	<property key="defaultColor" type="string" defaultValue="#00ffff">
            <caption>Default Color</caption>
            <category>Appearance</category>
            <description>When a new object with empty color attribute is created, the color attribute will be set to this color.</description>
    </property>
  		<property key="enableLogging" type="boolean" defaultValue="false">
		  <caption>Enable Logging</caption>
		  <category>Logging</category>
		  <description>The boolean can enable/disable logging on this widget for debugging purposes.</description>
		</property></properties>

</widget>
